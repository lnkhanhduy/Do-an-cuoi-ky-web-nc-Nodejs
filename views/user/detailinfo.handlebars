                                    </svg>
                                </span>
                            </div>
                            
                            {{!-- admin-transaction-history-content-table-show --}}
                            <div class="admin-transaction-history-content-table admin-transaction-history-content-table-show js-have-transactions">
                                <div class="table-responsive">
                                    <table class="table" id="history-table">
                                        <thead class="table-thead">
                                            <tr>
                                                <th class="table-thead-trading-code">Trading code</th>
                                                <th class="table-thead-user">Day trading</th>
                                                <th class="table-thead-phone">Status</th>
                                                <th class="table-thead-vnd">Amount of money - VND</th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                        {{#each result}}
                                            <tbody class="table-tbody">
                                                <tr>
                                                    <td class="table-tbody-td-id">{{{this.id}}}</td>
                                                    <td class="table-tbody-td-date">{{{this.date}}}</td>

                                                    {{#compare this.action 'eq' 1}}
                                                        <td>
                                                            <span class="table-tbody-td-status table-tbody-td-status-tranfer">Nạp tiền</span>
                                                        </td>
                                                    {{/compare}}
                                                    {{#compare this.action 'eq' 2}}
                                                        <td>
                                                            <span class="table-tbody-td-status table-tbody-td-status-withdraw">Rút tiền</span>
                                                        </td>
                                                    {{/compare}}
                                                    {{#compare this.action 'eq' 3}}
                                                        <td>
                                                            <span class="table-tbody-td-status table-tbody-td-status-withdraw">Chờ duyệt</span>
                                                        </td>
                                                    {{/compare}}
                                                    <td class="table-tbody-td-date">{{addDos this.money}}</td>
                                                    <td></td>
                                                </tr>
                                            </tbody>
                                        {{/each}}
                                    </table>
                                </div>
                            </div>
