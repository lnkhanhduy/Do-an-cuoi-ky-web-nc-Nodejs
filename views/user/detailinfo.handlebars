<div class="admin-content-page-grid">
        <div class="admin-content-page-row">
            <div class="admin-content-page-column-4">
                {{#each user}}
                    <div class="admin-detail-user">
                            <div class="admin-detail-user-header">
                                <img src="https://preview.keenthemes.com/metronic8/demo15/assets/media/avatars/300-1.jpg" class="admin-detail-user-img" alt="Image">
                                <h3 class="admin-detail-user-name">{{this.fullname}}</h3>
                            </div>
                            <div class="admin-detail-user-tab">
                                <span class="admin-detail-user-info admin-detail-user-tab-active">Details</span>
                                <span class="admin-detail-user-id-card">ID card</span>
                            </div>
                            <div class="admin-detail-user-content">
                                <div class="admin-detail-user-content-info">
                                    <div class="admin-detail-user-content-item">
                                        <label class="admin-detail-user-content-item-label">Email</label>
                                        <span class="admin-detail-user-content-item-name">{{this.email}}</span>
                                    </div>
                                    <div class="admin-detail-user-content-item">
                                        <label class="admin-detail-user-content-item-label">phone number</label>
                                        <span class="admin-detail-user-content-item-name">{{this.phone}}</span>
                                    </div>
                                    <div class="admin-detail-user-content-item">
                                        <label class="admin-detail-user-content-item-label">Date of birth</label>
                                        <span class="admin-detail-user-content-item-name">{{this.birthday}}</span>
                                    </div>
                                    <div class="admin-detail-user-content-item">
                                        <label class="admin-detail-user-content-item-label">Address</label>
                                        <span class="admin-detail-user-content-item-name">{{this.address}}</span>
                                    </div>
                                </div>
                                <div class="admin-detail-user-content-item-id-card">
                                    <div class="admin-detail-user-content-item">
                                        <label class="admin-detail-user-content-item-label">photo ID card (Front)</label>
                                        <img src="/{{{this.email}}}/images/{{{this.cmndfront.name}}}" class="admin-detail-user-img-id-card" alt="Image">
                                    </div>
                                    <div class="admin-detail-user-content-item">
                                        <label class="admin-detail-user-content-item-label">photo ID card (Back Side)</label>
                                        <img  src="/{{{this.email}}}/images/{{{this.cmndback.name}}}" class="admin-detail-user-img-id-card" alt="Image">
                                    </div>
                                </div>
                            </div>
                        {{!-- <div class="admin-detail-user-btn-change-password">
                            <button class="btn btn-bottom btn-change-password">change password</button>
                        </div> --}}
                    </div>
                {{/each}}
            </div>
            <div class="admin-content-page-column-8">
                <div class="admin-transaction-history">
                    <div class="admin-header-name-change-tab admin-transaction-history-header">
                        {{#if mess}}
                            <span class="admin-header-name admin-header-name-tab admin-transaction-header-title">Transaction history</span>
                            <span class="admin-header-name admin-header-name-tab admin-password-header-title admin-header-name-tab-active">change password</span>
                            <span class="admin-header-name admin-header-name-tab admin-update-id-card-header-title">update id card</span>
                        {{else}}
                            <span class="admin-header-name admin-header-name-tab admin-transaction-header-title admin-header-name-tab-active">Transaction history</span>
                            <span class="admin-header-name admin-header-name-tab admin-password-header-title">change password</span>
                            <span class="admin-header-name admin-header-name-tab admin-update-id-card-header-title">update id card</span>
                        {{/if}}
                        {{!-- <div class="admin-header-line"></div> --}}
                    </div>
                    {{#if mess}}
                        <div class="admin-transaction-history-content mr-24 admin-header-name-tab-pane">
                            <div class="admin-content-header">
                                <span class="admin-content-header-search">
                                    <input type="text" class="admin-content-header-search-input" placeholder="Search user">
                                    <?xml version="1.0" encoding="UTF-8"?>
                                    <svg class="admin-content-header-icon-search" xmlns="http://www.w3.org/2000/svg" id="Outline" viewBox="0 0 24 24" width="14" height="14">
